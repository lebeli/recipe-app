{
  "testRegex": "((\\.|/*.)(spec))\\.jsx?$",
  "setupFilesAfterEnv": ["<rootDir>/jest.setup.jsx"],
  "moduleNameMapper": {
    "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/__tests__/__mocks__/fileMock.jsx",
    "\\.(scss|less)$": "<rootDir>/src/__tests__/__mocks__/styleMock.jsx"
  }
}
